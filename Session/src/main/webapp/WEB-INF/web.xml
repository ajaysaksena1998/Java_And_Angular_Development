 "http://java.sun.com/dtd/web-app_2_3.dtd" >

<web-app>
  <display-name>Archetype Created Web Application</display-name>
  <filter>
  	<filter-name>filterdone</filter-name>
  	<display-name>filterdone</display-name>
  	<description></description>
  	<filter-class>filterwork.filterdone</filter-class>
  </filter>
  <filter-mapping>
  	<filter-name>filterdone</filter-name>
  	<url-pattern>/session_file</url-pattern>
  </filter-mapping>
  <servlet>
  	<servlet-name>session</servlet-name>
  	<display-name>session</display-name>
  	<description></description>
  	<servlet-class>session_attribute.session</servlet-class>
  </servlet>
  <servlet>
  	<servlet-name>session_file</servlet-name>
  	<display-name>session_file</display-name>
  	<description></description>
  	<servlet-class>session_attribute.session_file</servlet-class>
  </servlet>
  <servlet-mapping>
  	<servlet-name>session</servlet-name>
  	<url-pattern>/session</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
  	<servlet-name>session_file</servlet-name>
  	<url-pattern>/session_file</url-pattern>
  </servlet-mapping>
</web-app>
